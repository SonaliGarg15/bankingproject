{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "user"
    },
    "cloud": {
      "config": {
        "uri": "http://configservercontainersonali:8888"
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true, //service registers itself in eureka
      "serviceUrl": "http://eurekaserver:8761/eureka", //address of eureka server
      "ValidateCertificates": false
    },
    "instance": { //tells how our service should be registered in eureka
      "appName": "userservice", //with this name other services will be able to query for address of our service
      "hostName": "userservice", //name of host our service is running
      "port": "80", //port which our service is using
      "nonSecurePortEnabled": true,
      "securePortEnabled": false
      //"securePort": "5008" //port which our service is using
    }
  }
}
